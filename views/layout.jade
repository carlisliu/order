doctype html
html(lang='en')
    head
        title= title
        meta(charset='utf-8')
        meta(name='name', content='width=device-width')
        link(rel='stylesheet', href='/stylesheets/bootstrap.css')
        link(rel='stylesheet', href='/stylesheets/bootstrap-responsive.css')
        link(rel='stylesheet', href='/stylesheets/jquery.fancybox.css')
        link(rel='stylesheet', href='/stylesheets/style.css',media='all')
        link(rel='stylesheet', href='/stylesheets/print.css',media='print')
    body
        div.topbar
            div.container-fluid
                a.company(href='dashboard.html') Order Delivery
                form(action='#')
                    input(type='text',value='Order No or date...')
                ul.mini
                    li
                        a(href='#')
                            img(src='images/icons/fugue/gear.png', alt='')
                            |Settings
                    li
                        a(href='#')
                            img(src='images/icons/fugue/control-power.png', alt='')
                            |Logout
        div.breadcrumbs
            div.container-fluid
                ul.bread.pull-left
                    li
                        a(href='dashboard.html')
                            i.icon-home.icon-white
                    li
                        a(href='dashboard.html') Dashboard
        div.main
        div.container-fluid
            div.navi
                ul.main-nav
                    li
                        a.light(href='#')
                            div.ico
                                i.icon-home.icon-white
                            |Order
                    li
                        a.light(href='#')
                            div.ico
                                i.icon-resize-full.icon-white
                            |Customer
                    li
                        a.light.toggle-collapsed(href='#')
                            div.ico
                                i.icon-th-large.icon-white
                            |Category
                        ul.collapsed-nav.closed
                            li
                                a(href='datatables.html') Data Tables
                            li
                                a(href='plaintables.html') Plain Tables
                            li
                                a(href='mediatables.htmll') Media Tables
                    li
                        a.light(href='#')
                            div.ico
                                i.icon-tasks.icon-white
                            |Product
            block content